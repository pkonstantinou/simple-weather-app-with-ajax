{
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "no-console": "off",
      "no-alert": "off",
      "comma-dangle": "off",
      "quotes": "off",
      "no-unused-vars": "off",
      "no-param-reassign": "off",
      "class-methods-use-this": "off",
      "import/prefer-default-export": "off",
      "prefer-destructuring": "off"
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "parser": "babel-eslint"
  }